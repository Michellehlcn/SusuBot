{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "functions": {
        "api/webhook.js": {
          "includeFiles": "srcfix vercel/**"
        }
      },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/server.js"
      }
    ]
  }