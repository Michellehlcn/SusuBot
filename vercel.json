{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["src/views/**"] }
      }
    ],
    "routes": [
      {
        "src": "/api/webhook(.*)",
        "dest": "src/server.js"
      },
      {
        "src": "/",
        "dest": "src/views/homepage.ejs"
      }
    ]
  }