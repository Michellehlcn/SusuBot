{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
     "functions": {
          "src/server.js": {
            "includeFiles": "src/views/**"
          }
        },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/server.js"
      }
    ]
  }